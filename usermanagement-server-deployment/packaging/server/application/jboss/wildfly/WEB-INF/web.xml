<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	
	<display-name>User Management Server</display-name>
	
	<!-- System -->
	
	<context-param>
		<param-name>cyk.variable.system.identifier</param-name>
		<param-value>usermanagement.server</param-value>
	</context-param>
	
	<context-param>
		<param-name>cyk.variable.system.name</param-name>
		<param-value>SIIBC Gestion des acteurs</param-value>
	</context-param>
	
	<context-param>
		<param-name>cyk.variable.system.web.context</param-name>
		<param-value>${jboss-web.context-root}</param-value>
	</context-param>	
	
	<!--  -->
	
	<context-param>
		<param-name>cyk.variable.ci.gouv.dgbf.system.usermanagement.server.persistence.entities.account.role.Scope-SECTION-uri</param-name>
		<param-value>http://localhost:3000/section</param-value>
	</context-param>
	
	<context-param>
		<param-name>cyk.variable.ci.gouv.dgbf.system.usermanagement.server.persistence.entities.account.role.Scope-SECTION-code</param-name>
		<param-value>numero</param-value>
	</context-param>
	
	<context-param>
		<param-name>cyk.variable.ci.gouv.dgbf.system.usermanagement.server.persistence.entities.account.role.Scope-SECTION-name</param-name>
		<param-value>libelleLong</param-value>
	</context-param>
	
	<context-param>
		<param-name>cyk.variable.ci.gouv.dgbf.system.usermanagement.server.persistence.entities.account.role.Scope-SECTION-link</param-name>
		<param-value>uuid</param-value>
	</context-param>
	
	<!-- Swagger -->
	
	<context-param>
		<param-name>cyk.variable.swagger.enabled</param-name>
		<param-value>true</param-value>
	</context-param>
	
	<context-param>
		<param-name>cyk.variable.swagger.bean.config.resource.package</param-name>
		<param-value>org.cyk.utility.server.representation.impl,ci.gouv.dgbf.system.usermanagement.server.representation.api</param-value>
	</context-param>
		
</web-app>